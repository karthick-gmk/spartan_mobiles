{
    "version":2,
    "builds":[
        {   "src":"myapp/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"15mb","runtime":"python3.9"}
        },{
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles_bulid"
            }
        }
           
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"staticfiles_bulid/$1"
        },
        {
            "src":"/(.*)",
            "dest":"myapp/wsgi.py"
        }
    ]
}