{
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "myapp.settings"
  },
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "myapp.settings"
    }
  }
}